$(document).ready(function() {

     var div = document.getElementById('currentMatch');

if (div.innerText > 32) {
    newColor = "green";
    alert(div.innerText);
} else {
    newColor = "red";
}

color = getComputedStyle(div, null).color;
alert(color);

alert('Selon le Javascript, la couleur de fond de ce <div> est : '+ div.style.backgroundColor);
div.element.style.fontcolor = newColor;

});