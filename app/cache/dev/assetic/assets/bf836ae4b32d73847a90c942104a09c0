$(document).ready(function() {

     var div = document.getElementById('currentMatch');
     var divv = document.getElementTagName('td');

     for (var i = 0 ; i < divvLength ; i++) {
if (div.innerText > 32) {
    newColor = "green";
} else {
    newColor = "red";
}
    div.style.color = newColor;
}

});